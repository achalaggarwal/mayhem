<!-- Adds the Filepicker.io javascript library to the page -->
<script src="https://api.filepicker.io/v0/filepicker.js"></script>
<script type="text/javascript">
    //Seting up Filepicker.io with your api key
    filepicker.setKey('AUS33DYKaTl6eYH4HJC73z');
</script>

<script>
$('#upload').bind('click', function() {
  filepicker.getFile('*/*', function(url, data){
    $.ajax({
      type: 'POST',
      url: '/upload',
      data: {data: data, url: url},
      success: function(data) { $('body').append(data); }
    });
  });
});

function selectTab(me, tab) {
  $('#designer-content').hide();
  $('#developer-content').hide();
  $($(me).parent().parent().children()[0]).children().removeClass('active');
  $($(me).parent().parent().children()[1]).children().removeClass('active');
  $(me).addClass('active');
  switch(tab) {
    case 0:
      $('#developer-content').show();
    break;
    case 1:
      $('#designer-content').show();
    break;
  }
}

</script>
<style>
.active {
  text-decoration: underline !important;
}
</style>
